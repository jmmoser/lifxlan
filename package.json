{
  "name": "lifxlan",
  "version": "0.0.71",
  "description": "LIFX LAN protocol in TypeScript",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "type": "module",
  "unpkg": "./dist/lifxlan.min.js",
  "scripts": {
    "test": "bun test --coverage",
    "node:test": "node --test --experimental-test-coverage",
    "build": "tsc && bun build ./dist/index.js --outdir ./dist --minify --entry-naming [dir]/lifxlan.min.[ext]",
    "lint": "oxlint",
    "benchmark": "bun run benchmarks/performance.js",
    "prepack": "bun run build && bun test"
  },
  "keywords": [
    "lifx",
    "lan"
  ],
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmmoser/lifxlan.git"
  },
  "author": "Justin Moser",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "^1.2.2",
    "@types/node": "^22.13.0",
    "mitata": "^1.0.34",
    "oxlint": "^0.15.9",
    "typescript": "^5.7.3"
  }
}
